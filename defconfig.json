{
  "markup_languages": ["HTML", "Markdown", "LaTeX"],

  "extensions": {
    "HTML": ["html", "htm"],
    "Markdown": ["md", "markdown", "mdown", "mkdn", "mkd"],
    "LaTeX": ["tex"]
  },

  "output": {
    "filename": "{I_NAME}{I_EXT}_out{O_EXT}",
    "patterns": {
      "input_name": "{I_NAME}",
      "input_extension": "{I_EXT}",
      "output_extension": "{O_EXT}"
    }
  },

  "execution_timeouts": {
    "default": 1000,

    "languages": {
      "Python 3": 200
    }
  },

  "execution_results": {
    "HTML": {
      "image": "<img src=\"{SRC}\">",
      "source": "<pre>{CODE}</pre>",
      "result": "<p>{RES}</p>",

      "patterns": {
        "image": "{SRC}",
        "source": "{CODE}",
        "result": "{RES}"
      }
    }
  },

  "code_snippets": {
    "default": {
      "begin": "<#{S}>",
      "end": "<@>",
      "output": "<${S}>",
      "separators": [",", " "],

      "settings": {
        "language": "[{L}]",
        "echo": "echo={E}",
        "output": "output={O}",
        "context": "context={C}",
        "snippet_id": "id={I}",
        "timeout": "timeout={T}",
        "error": "allow_error={R}",
        "output_type": "output_type={OT}"
      },

      "patterns": {
        "settings": "{S}",
        "language": "{L}",
        "echo": "{E}",
        "output": "{O}",
        "context": "{C}",
        "snippet_id": "{I}",
        "timeout": "{T}",
        "error": "{R}",
        "output_type": "{OT}"
      }
    },

    "HTML": {
      "begin": "<snippet{S}>",
      "end": "</snippet>",
      "output": "<output{S} />",

      "settings": {
        "language": "lang=\"{L}\"",
        "echo": "echo=\"{E}\"",
        "output": "output=\"{O}\"",
        "context": "context=\"{C}\"",
        "snippet_id": "id=\"{I}\"",
        "timeout": "timeout=\"{T}\"",
        "error": "allow_error=\"{R}\"",
        "output_type": "output_type=\"{OT}\""
      },

      "patterns": {
        "settings": "{S}",
        "language": "{L}",
        "echo": "{E}",
        "output": "{O}",
        "context": "{C}",
        "snippet_id": "{I}",
        "timeout": "{T}",
        "error": "{R}",
        "output_type": "{OT}"
      }
    }
  }
}